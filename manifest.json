{
	"name": "Screen Recorder",
	"description": "Screen recording chrome extension task 4",
	"version": "4.0.0",
	"manifest_version": 3,
	"permissions": ["desktopCapture", "activeTab", "scripting", "storage"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": "icon.png"
	}
}
